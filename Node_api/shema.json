            "type": "string"
        },
        "firstName": {
            "description": "Entrepreneur's last name",
            "type": "string"
        },
        "email": {
            "description": "Entrepreneur's email",
            "type": "string"
        },
        "phoneNumber": {
            "description": "Entrepreneur's phone number",
            "type": "integer"
